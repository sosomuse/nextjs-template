  "dependencies": {
    "next": "13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.18.4",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
